{
  "name": "@lykmapipo/predefine",
  "version": "0.8.1",
  "description": "A representation of stored and retrieved information that does not qualify to belongs to their own domain model.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js examples/",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit --timeout=8000 --require @babel/register test/**/*.spec.js",
    "test:unit": "NODE_ENV=test npm run pretest && NODE_ENV=test mocha --exit --timeout=8000 --require @babel/register test/unit/**/*.spec.js",
    "test:integration": "NODE_ENV=test npm run pretest && NODE_ENV=test mocha --exit --timeout=8000 --require @babel/register test/integration/**/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog",
    "chore:deps": "ncu -u && npm install --save",
    "lint:config": "eslint --print-config rollup.config.js",
    "dev": "npm run seed && npm run start",
    "start": "NODE_ENV=development BASE_PATH=./examples node ./examples/app.js",
    "seed": "NODE_ENV=development BASE_PATH=./examples node ./examples/seed.js"
  },
  "keywords": [
    "lykmapipo",
    "mongoose",
    "mongoose-module",
    "predefined",
    "predefine",
    "settings",
    "config",
    "env",
    "preference"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lykmapipo/predefine.git"
  },
  "author": {
    "name": "lykmapipo",
    "email": "lallyelias87@gmail.com",
    "url": "https://github.com/lykmapipo"
  },
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/predefine/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/lykmapipo/predefine",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@benmaruchu/faker": "^4.3.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/travis-cli": "^8.2.0",
    "@lykmapipo/express-test-helpers": "^0.8.7",
    "@lykmapipo/mongoose-test-helpers": "^0.7.19",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.6",
    "cz-conventional-changelog": "^3.0.2",
    "doxdox": "^3.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.9.2",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "generate-changelog": "^1.8.0",
    "handlebars": "^4.3.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "lodash.template": "^4.5.0",
    "mocha": "^6.2.0",
    "mongoose": ">=5.7.1",
    "npm-check-updates": "^3.1.23",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "shelljs": "^0.8.3"
  },
  "peerDependencies": {
    "mongoose": ">=5.7.1"
  },
  "dependencies": {
    "@lykmapipo/common": ">=0.26.4",
    "@lykmapipo/env": ">=0.14.1",
    "@lykmapipo/express-common": ">=0.17.11",
    "@lykmapipo/express-rest-actions": ">=0.6.8",
    "@lykmapipo/mongoose-common": ">=0.25.13",
    "@lykmapipo/mongoose-exportable": ">=0.3.8",
    "async": ">=3.1.0",
    "lodash": ">=4.17.15",
    "mongoose-geojson-schemas": ">=0.10.10",
    "mongoose-locale-schema": ">=0.4.1",
    "mongoose-rest-actions": ">=0.28.9"
  },
  "engines": {
    "node": ">=12.4.0",
    "npm": ">=6.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "git add -A"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
